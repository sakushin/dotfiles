# for GPD Pocket
include ".mayu.us"

# Z列補足
key *FullStop = &Ignore
key *Solidus = *FullStop
mod shift -= RightShift
key *RightShift = *Solidus

# A列補足
key *Delete = *Semicolon
key *BackSpace = *Quote

# Q列補足
mod control -= RightControl
key *RightControl = *LeftSquareBracket
key *Semicolon = *RightSquareBracket
key *Quote = *ReverseSolidus

# 1列補足
key M2-*Enter = *_0
key M2-*Delete = *HyphenMinus
key M2-*BackSpace = *EqualsSign

if (!LINUX)
  key *Applications = *MButton
endif

